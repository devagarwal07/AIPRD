    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.4",
    "vitest": "^3.2.4",
    "prettier": "^3.3.3",
    "vite-bundle-visualizer": "^0.9.4"
